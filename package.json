{
  "name": "gameuicomponents",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "scripts": {
    "start:demo": "webpack-dev-server",
    "build": "node ./scripts/build.js",
    "link": "node ./scripts/link.js",
    "unlink": "node ./scripts/unlink.js",
    "build:dev": "npm run clean && npm run link && npm run build -- -ni && npm run build:demo && npm run check:copyright",
    "build:library": "node ./scripts/build.js --library",
    "build:documentation": "node ./scripts/build.js --documentation",
    "check:copyright": "node ./scripts/copyright-notice.js",
    "add:copyright": "node ./scripts/copyright-notice.js -a",
    "clean": "node ./scripts/clean.js",
    "pack": "node ./scripts/pack.js",
    "pack:library": "node ./scripts/pack.js --directory ./lib",
    "build:demo": "node ./scripts/build-demo.js",
    "install:all": "npm-recursive-install --rootDir=components",
    "rebuild": "npm i && npm run clean && npm run build:library && npm run pack:library && npm run build && npm run build:demo",
    "test": "node ./scripts/test.js",
    "test:Chrome": "node ./scripts/test.js -- --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoherentLabs/GameUIComponents.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CoherentLabs/GameUIComponents/issues"
  },
  "homepage": "https://github.com/CoherentLabs/GameUIComponents#readme",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "css-loader": "^5.2.7",
    "karma": "^6.3.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^5.0.0",
    "recursive-install": "^1.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "^2.0.0",
    "uglifycss": "0.0.29",
    "webpack": "^5.59.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  },
  "engines": {
    "node": ">=16.10.0"
  }
}
